{
  "name": "@apps/emis-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "eslint . --fix --max-warnings=0",
    "lokalise:down": "node ../../tools/scripts/lokalise.js down emis-app",
    "lokalise:up": "node ../../tools/scripts/lokalise.js up emis-app",
    "lokalise:upErrors": "node scripts/lokalise.js upErrors",
    "lokalise": "yarn lokalise:up && yarn lokalise:upErrors && yarn lokalise:down",
    "lokalise:delete": "node ../../tools/scripts/lokalise.js delete benef-app -file ../../tools/scripts/i18n_keys_to_delete.txt -path src/ui/lang",
    "test:e2e:headless": "cross-env VUE_APP_MOCK_AUTHENTICATION=true testcafe chrome:headless --reporter spec,xunit:report.xml",
    "debug:e2e": "cross-env VUE_APP_MOCK_AUTHENTICATION=true testcafe $NODE_DEBUG_OPTION chrome",
    "serve:e2e": "cross-env VUE_APP_MOCK_AUTHENTICATION=true yarn serve",
    "test:e2e": "cross-env VUE_APP_MOCK_AUTHENTICATION=true testcafe chrome --live"
  },
