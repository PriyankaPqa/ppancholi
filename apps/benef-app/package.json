    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.0",
    "sass": "1.32.13",
    "start-server-and-test": "^1.15.2",
    "tsconfig-paths": "^4.1.1",
    "unplugin-vue-components": "^0.22.12",
    "vite-plugin-eslint": "^1.8.1",
