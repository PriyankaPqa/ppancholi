    "@types/lodash": "4.14.179",
    "@vue/test-utils": "^1.3.4",
    "cypress": "^12.11.0",
    "cypress-cloud": "^1.7.4",
    "cypress-multi-reporters": "^1.6.2",
    "cypress-repeat": "^2.3.3",
    "cypress-terminal-report": "^5.0.0",
    "cypress-xpath": "^2.0.1",
