    "@libs/shared-lib": "*",
    "@libs/stores-lib": "*",
    "@types/uuid": "8.3.4",
    "@vitejs/plugin-vue2": "^2.1.0",
    "awesome-phonenumber": "2.68.0",
    "axios": "0.27.2",
    "camelcase-keys": "^6.2.2",
    "deepmerge": "^4.2.2",
    "lodash": "^4.17.21",
    "date-fns": "2.28.0",
    "odata-query": "^6.7.0",
    "pinia": "^2.0.27",
    "register-service-worker": "^1.7.1",
    "survey-core": "1.9.58",
    "survey-creator-core": "1.9.58",
    "survey-creator-knockout": "1.9.58",
    "survey-knockout-ui": "1.9.58",
    "survey-vue": "1.9.58",
    "uuid": "^8.3.2",
    "vee-validate": "^3.3.9",
