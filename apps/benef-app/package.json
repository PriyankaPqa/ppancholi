{
  "name": "@apps/benef-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint . --fix --max-warnings=0",
    "lokalise:down": "node ../../tools/scripts/lokalise.js down benef-app",
    "lokalise:up": "node ../../tools/scripts/lokalise.js up benef-app",
    "lokalise": "yarn lokalise:up && yarn lokalise:down",
    "lokalise:delete": "node ../../tools/scripts/lokalise.js delete benef-app -file ../../tools/scripts/i18n_keys_to_delete.txt -path src/ui/lang",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info",
    "cy:version": "cypress version",
    "cy:cache:list": "cypress cache list",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "cy:run:smoke": "yarn cy:run --spec 'cypress/e2e/smoke-tests/**/*.ts'",
    "cy:run:critical": "yarn cy:run --spec 'cypress/e2e/critical-path-tests/**/*.ts'",
    "cy:open": "cypress open",
    "cy:delete:reports": "rm cypress/results/* || true",
    "cy:combine:reports": "jrm cypress/results/combined-report.xml \"cypress/results/*.xml\"",
    "cy:report": "yarn cy:delete:reports & yarn cypress-cloud --reporter cypress-multi-reporters --reporter-options configFile=cypress-reporter-config.js"
  },
  "dependencies": {
    "@libs/component-lib": "*",
