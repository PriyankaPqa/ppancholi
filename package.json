    "jest-transform-stub": "^2.0.0",
    "jest-trx-results-processor": "^2.2.0",
    "lerna": "^4.0.0",
    "lerna-update-wizard": "^1.1.1",
    "lint-staged": "15.2.10",
    "node-gyp": "10.0.1",
    "prompt-sync": "^4.2.0",
    "regenerator-runtime": "^0.13.9",
    "simple-git": "^3.18.0",
    "ts-jest": "29.2.5",
    "typescript": "~4.5.5",
    "vue": "2.7.14",
    "vue-template-compiler": "^2.7.14",
    "weak-napi": "^2.0.2",
    "yarn-run-all": "^3.1.1"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean",
    "clean:all": "rm -rf node_modules && lerna clean",
    "clean:all:windows": "del node_modules -force && lerna clean",
    "lerna:list": "lerna list --all",
    "test": "jest  --runInBand --logHeapUsage",
    "lint": "yarn workspaces run lint",
    "lerna:update": "lernaupdate",
    "test:parallel": "node ./tools/scripts/parallelize-test.js",
    "emis-app:build": "yarn --cwd apps/emis-app build",
    "emis-app:serve": "yarn --cwd apps/emis-app serve",
    "emis-app:dev": "yarn --cwd apps/emis-app dev",
    "emis-app:lab": "yarn emis-app:dev --mode lab",
    "emis-app:dev:lab": "yarn emis-app:dev --mode lab",
    "emis-app:dev:local": "yarn emis-app:dev --mode local_development",
    "emis-app:lab:local": "yarn emis-app:dev --mode local_lab",
    "emis-app:lint": "yarn --cwd apps/emis-app lint",
    "emis-app:test": "yarn jest --projects apps/emis-app --runInBand --logHeapUsage",
    "emis-app:test:graph": "jest-heap-graph 'yarn emis-app:test --logHeapUsage --no-cache'",
    "emis-app:i18n-report": "node tools/scripts/i18n-report.js 'apps/emis-app/src/ui/lang/emis/en.json','apps/emis-app/src/ui/lang/registration/en.json' 'apps/emis-app/src'",
    "emis-app:lokalise": "yarn --cwd apps/emis-app lokalise",
    "emis-app:cy:run": "yarn --cwd apps/emis-app cypress run",
    "emis-app:cy:open": "yarn --cwd apps/emis-app cypress open --config watchForFileChanges=false",
    "benef-app:build": "yarn --cwd apps/benef-app build",
    "benef-app:serve": "yarn --cwd apps/benef-app serve",
    "benef-app:dev": "yarn --cwd apps/benef-app dev",
    "benef-app:lab": "yarn benef-app:dev --mode lab",
    "benef-app:dev:lab": "yarn benef-app:dev --mode lab",
    "benef-app:dev:local": "yarn benef-app:dev --mode localhost",
    "benef-app:lint": "yarn --cwd apps/benef-app lint",
    "benef-app:test": "yarn jest --projects apps/benef-app --maxWorkers=50%",
    "benef-app:test:graph": "jest-heap-graph 'yarn benef-app:test --logHeapUsage --no-cache'",
    "benef-app:i18n-report": "node tools/scripts/i18n-report.js 'apps/benef-app/src/ui/lang/en.json' 'apps/emis-app/src'",
    "benef-app:lokalise": "yarn --cwd apps/benef-app lokalise",
    "benef-app:cy:run": "yarn --cwd apps/benef-app cypress run",
    "benef-app:cy:open": "yarn --cwd apps/benef-app cypress open --config watchForFileChanges=false",
    "registration-lib:lint": "yarn --cwd libs/registration-lib lint",
    "registration-lib:test": "yarn jest --projects libs/registration-lib --maxWorkers=50%",
    "registration-lib:test:graph": "jest-heap-graph 'yarn registration-lib:test --logHeapUsage --no-cache'",
    "registration-lib:i18n-report": "node tools/scripts/i18n-report.js 'libs/registration-lib/src/ui/lang/en.json' 'libs/registration-lib/src'",
    "component-lib:lint": "yarn --cwd libs/component-lib lint",
    "component-lib:test": "yarn jest --projects libs/component-lib --maxWorkers=50%",
    "component-lib:test:graph": "jest-heap-graph 'yarn component-lib:test --logHeapUsage --no-cache'",
    "shared-lib:lint": "yarn --cwd libs/shared-lib lint",
    "shared-lib:test": "yarn jest --projects libs/shared-lib --maxWorkers=50%",
    "shared-lib:test:graph": "jest-heap-graph 'yarn shared-lib:test --logHeapUsage --no-cache'",
    "entities-lib:lint": "yarn --cwd libs/entities-lib lint",
    "entities-lib:test": "yarn jest --projects libs/entities-lib --maxWorkers=50%",
    "entities-lib:test:graph": "jest-heap-graph 'yarn entities-lib:test --logHeapUsage --no-cache'",
    "services-lib:lint": "yarn --cwd libs/services-lib lint",
    "services-lib:test": "yarn jest --projects libs/services-lib --maxWorkers=50%",
    "services-lib:test:graph": "jest-heap-graph 'yarn services-lib:test --logHeapUsage --no-cache'",
    "stores-lib:lint": "yarn --cwd libs/stores-lib lint",
    "stores-lib:test": "yarn jest --projects libs/stores-lib --maxWorkers=50%",
    "stores-lib:test:graph": "jest-heap-graph 'yarn stores-lib:test --logHeapUsage --no-cache'",
    "prepare": "husky install",
    "fb": "node tools/scripts/feature-branch.js"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "libs/*"
    ]
  },
  "lint-staged": {
    "**/*.+(js|jx|ts|tsx|vue)": [
      "eslint --fix --max-warnings=0 --cache"
    ]
  },
  "resolutions": {
    "minimist": "1.2.8"
  },
  "dependencies": {}
}
