{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build:all": "yarn lokalise && yarn build",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit --runInBand",
    "test:clearCache": "yarn test --clearCache",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "lint": "vue-cli-service lint",
    "dev": "(vue-cli-service lint) && (vue-cli-service serve --open)",
    "inspect": "vue-cli-service inspect",
    "install:clean": "rm -rf node_modules/ && yarn install",
    "lint:fix": "vue-cli-service lint --fix",
    "lokalise:down": "node lokalise.js down",
    "lokalise:up": "node lokalise.js up",
    "lokalise": "yarn lokalise:up && yarn lokalise:upErrors && yarn lokalise:down",
    "lokalise:upErrors": "node lokalise.js upErrors",
    "report": "vue-cli-service build --report",
    "report-json": "vue-cli-service build --report-json",
    "coverage": "vue-cli-service test:unit --coverage --coverageReporters=cobertura",
    "pr": "yarn test && yarn lint",
    "link:registration": "yalc link @crctech/registration-lib",
    "remove:registration": "yalc remove @crctech/registration-lib"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.19.0",
    "@crctech/component-library": "1.0.153",
    "@crctech/registration-lib": "1.0.549",
    "@mdi/font": "^6.4.95",
    "@microsoft/applicationinsights-web": "^2.7.0",
    "@microsoft/signalr": "^5.0.11",
    "@types/uuid": "^8.3.0",
    "awesome-phonenumber": "^2.64.0",
    "axios": "^0.24.0",
    "core-js": "^3.19.1",
    "deepmerge": "^4.2.2",
    "dotenv": "^10.0.0",
    "fstream": "^1.0.12",
    "js-convert-case": "^4.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.0",
    "register-service-worker": "^1.7.1",
    "roboto-fontface": "^0.10.0",
    "sanitize-html": "^2.5.3",
    "uuid": "^8.3.2",
    "vee-validate": "^3.3.9",
    "vue": "^2.6.12",
    "vue-i18n": "^8.26.7",
    "vue-meta": "^2.3.1",
    "vue-router": "^3.5.3",
    "vue-toasted": "^1.1.27",
    "vue2-transitions": "^0.3.0",
    "vuedraggable": "^2.24.1",
