{
  "name": "@crctech/registration-lib",
  "version": "0.1.118",
  "private": false,
  "watch": {
    "push": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "js,ts",
      "ignore": "",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2500,
      "runOnChangeOnly": true
    }
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "test": "vue-cli-service test:unit --runInBand",
    "lint": "vue-cli-service lint --fix",
    "lokalise": "yarn lokalise:up && yarn lokalise:down",
    "lokalise:down": "node lokalise.js down",
    "lokalise:up": "node lokalise.js up",
    "build:lib:webpack": "vue-cli-service build --target lib --name registration-lib src/entry.ts",
    "build:all": "yarn build:lib && yarn build:app",
    "build:app": "vue-cli-service build --dest dist-app",
    "build:final": "yarn lint && yarn test && yarn build:lib",
    "build:lib": "rollup --config build/rollup.config.js",
    "build:test": "yarn build:lib && yarn pack",
    "coverage": "vue-cli-service test:unit --coverage --coverageReporters=cobertura",
    "pre": "yarn build:lib && yarn version --prerelease",
    "patch": "yarn build:lib && yarn version --patch",
    "pr": "yarn test && yarn lint",
    "watch": "npm-watch",
    "push": "yarn build:lib && yalc push"
  },
  "main": "dist/registration-lib.ssr.js",
  "browser": "dist/registration-lib.esm.js",
  "module": "dist/registration-lib.esm.js",
  "unpkg": "dist/registration-lib.min.js",
  "types": "src/registration-lib.d.ts",
  "files": [
    "src/types/**",
    "src/components",
    "src/registration-lib.d.ts",
    "src/helpers.ts",
    "src/entities",
    "src/store/**",
    "src/constants/**",
    "src/ui/**",
    "src/services/**",
    "dist",
    "LICENSE",
    "README.md",
    "typings/**"
  ],
  "dependencies": {
    "@crctech/component-library": "1.0.14",
    "axios": "^0.21.1",
    "camelcase-keys": "^6.2.2",
    "moment": "^2.29.1",
    "odata-query": "^6.7.0",
    "uuid": "^8.3.2",
    "vuex": "^3.6.2"
  },
  "peerDependencies": {
    "vue": "^2.6.11",
    "vuetify": "^2.4.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-typescript": "^7.13.0",
    "@lokalise/node-api": "^2.0.1",
    "@mdi/font": "^5.6.55",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^8.1.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@types/jest": "^24.0.19",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "awesome-phonenumber": "^2.49.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-vue": "^2.0.2",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "eslint": "^7.23.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.8.0",
    "eslint-plugin-vuejs-accessibility": "^0.6.0",
    "eslint-plugin-vuetify": "^1.0.0-beta.7",
    "flush-promises": "^1.0.2",
    "jest-trx-results-processor": "^2.2.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "node-sass": "^5.0.0",
    "npm-watch": "^0.9.0",
    "postcss": "^8.2.4",
    "rollup": "^2.30.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-bundle-scss": "^0.1.2",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "rollup-plugin-vuetify": "^0.2.4",
    "sass": "^1.26.11",
    "sass-loader": "^10.1.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.0.5",
    "unzipper": "^0.10.11",
    "vee-validate": "^3.4.5",
    "vue": "^2.6.12",
    "vue-cli-plugin-vuetify": "~2.0.9",
    "vue-i18n": "^8.21.1",
    "vue-template-compiler": "^2.6.12",
    "vuetify": "^2.4.2",
    "vuetify-loader": "^1.6.0"
  }
}
